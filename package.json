{
  "name": "helperai",
  "version": "0.3.0",
  "description": "Help customer supporters with drafted answers and actions.",
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run-on-packages build && next build",
    "build:preview": "pnpm run build && pnpm run db:prod:migrate && pnpm tsx --conditions=react-server ./db/seeds/index.ts",
    "clean": "git clean -xdf .cache .next node_modules",
    "deploy": "pnpm run build && pnpm run db:prod:migrate",
    "db:check": "pnpm services:start && pnpm run with-dev-env drizzle-kit check --config ./db/drizzle.config.ts",
    "db:generate": "pnpm services:start && pnpm run with-dev-env drizzle-kit generate --config ./db/drizzle.config.ts",
    "db:migrate": "pnpm services:start && pnpm run with-dev-env drizzle-kit migrate --config ./db/drizzle.config.ts && pnpm db:setup-cron",
    "db:prod:migrate": "drizzle-kit migrate --config ./db/drizzle.config.ts && pnpm db:prod:setup-cron",
    "db:setup-cron": "pnpm run with-dev-env pnpm db:prod:setup-cron",
    "db:prod:setup-cron": "pnpm tsx --conditions=react-server ./db/setupCron.ts",
    "db:seed": "pnpm services:start && pnpm run with-dev-env pnpm tsx --conditions=react-server ./db/seeds/seedDatabase.ts",
    "db:clear": "pnpm services:start && pnpm supabase db reset && pnpm db:migrate",
    "db:reset": "pnpm db:clear && pnpm db:seed",
    "db:delete-saved-replies": "pnpm run with-dev-env pnpm tsx --conditions=react-server ./scripts/delete-saved-replies.ts",
    "dev": "bash scripts/dev.sh",
    "dev:email": "pnpm run with-dev-env email dev --dir lib/emails --port 3060",
    "dev:next": "lsof -ti:3010 | xargs kill -9 || true && pnpm next dev --turbopack -H 0.0.0.0",
    "dev:test": "pnpm run with-dev-env next dev --port 3010 --turbopack",
    "eval:dev": "EVALITE=true NODE_ENV=test pnpm run with-dev-env evalite watch",
    "evals": "EVALITE=true NODE_ENV=test pnpm run with-dev-env evalite",
    "format": "pnpm run-on-packages format && prettier --check . --ignore-path ./.gitignore --ignore-path ./.prettierignore",
    "lint": "pnpm run-on-packages lint && eslint",
    "repl": "pnpm run with-dev-env pnpm tsx --conditions=react-server --import scripts/replGlobals.mts",
    "repl:preview": "bash scripts/with-vercel-env.sh preview pnpm tsx --conditions=react-server --import scripts/replGlobals.mts",
    "repl:prod": "bash scripts/with-vercel-env.sh production pnpm tsx --conditions=react-server --import scripts/replGlobals.mts",
    "start": "pnpm next start",
    "test": "pnpm run-on-packages test && pnpm test:unit",
    "test:unit": "pnpm run with-dev-env vitest",
    "test:watch": "pnpm test:unit --watch",
    "test:e2e": "pnpm run db:delete-saved-replies && pnpm run with-dev-env playwright test",
    "test:e2e:clean": "pnpm run db:delete-saved-replies && pnpm run with-dev-env playwright test",
    "test:e2e:with-data": "pnpm run with-dev-env playwright test",
    "test:e2e:debug": "pnpm run db:delete-saved-replies && pnpm run with-dev-env playwright test --debug",
    "test:e2e:headed": "pnpm run with-dev-env playwright test --headed",
    "test:e2e:headed:clean": "pnpm run db:delete-saved-replies && pnpm run with-dev-env playwright test --headed",
    "test:e2e:setup": "pnpm run with-dev-env playwright install",
    "typecheck": "pnpm run with-dev-env pnpm run build && pnpm run-on-packages typecheck && pnpm run with-dev-env tsc --noEmit && pnpm run db:check",
    "services:start": "pnpm with-dev-env pnpm supabase start && (docker ps | grep helperai-nginx || pnpm nginx:start)",
    "services:stop": "pnpm supabase stop && docker stop helperai-nginx || true",
    "nginx:start": "docker run -d --rm --name helperai-nginx -p 80:80 -p 443:443 --add-host host.docker.internal:host-gateway -v \"$(pwd)/scripts/docker/local-nginx/helperai_dev.conf:/etc/nginx/conf.d/default.conf\" -v \"$(pwd)/scripts/docker/local-nginx/certs:/etc/ssl/certs\" nginx:1.27.5",
    "generate-ssl-certificates": "bash scripts/generate-ssl-certificates.sh",
    "run-on-packages": "pnpm run --filter=!helperai --if-present --workspace-concurrency=1",
    "with-dev-env": "dotenv -e ./.env.development.local -e ./.env.local --"
  },
