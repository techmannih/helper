{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "!README.md"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "deploy": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "test": {
      "dependsOn": [],
      "outputs": [".cache/.vitest"]
    },
    "test:coverage": {
      "dependsOn": []
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "ABLY_API_KEY",
    "POSTGRES_URL",
    "POSTGRES_URL_NON_POOLING",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "GOOGLE_PUBSUB_TOPIC_NAME",
    "HELPER_API_KEY",
    "AUTH_SECRET",
    "AUTH_URL",
    "NEXT_RUNTIME",
    "NODE_ENV",
    "PORT",
    "SLACK_CLIENT_ID",
    "SLACK_CLIENT_SECRET",
    "STRIPE_PRICE_ID",
    "STRIPE_WEBHOOK_SECRET",
    "STRIPE_SECRET_KEY",
    "WIDGET_JWT_SECRET",
    "CRYPTO_SECRET",
    "ENCRYPT_COLUMN_SECRET",
    "RESEND_API_KEY",
    "AWS_ACCESS_KEY_ID",
    "AWS_SECRET_ACCESS_KEY",
    "AWS_DEFAULT_REGION",
    "AWS_PRIVATE_STORAGE_BUCKET_NAME",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "GOOGLE_PUBSUB_CLAIM_EMAIL",
    "SLACK_SIGNING_SECRET",
    "DEMO_MAILBOX_SLUG",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY",
    "NEXT_PUBLIC_CLERK_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_SIGN_UP_URL",
    "CLERK_SIGN_IN_FALLBACK_REDIRECT_URL",
    "CLERK_SIGN_UP_FALLBACK_REDIRECT_URL",
    "ADDITIONAL_PAID_ORGANIZATION_IDS",
    "JINA_API_TOKEN",
    "LANGFUSE_SECRET_KEY",
    "LANGFUSE_PUBLIC_KEY",
    "LANGFUSE_BASEURL",
    "KV_UPSTASH_KV_REST_API_URL",
    "KV_UPSTASH_KV_REST_API_TOKEN",
    "OPENAI_API_KEY",
    "FIRECRAWL_API_KEY",
    "PROXY_URL",
    "PROXY_SECRET_KEY",
    "GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET"
  ],
  "globalPassThroughEnv": [
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "npm_lifecycle_event",
    "TURBO_TOKEN",
    "TURBO_TEAM",
    "DRIZZLE_LOGGING",
    "CLERK_INITIAL_ORGANIZATION_ID",
    "CLERK_INITIAL_USER_IDS"
  ]
}
